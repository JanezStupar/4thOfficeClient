<!--<?xml version="1.0" encoding="utf-8"?>-->
<!-- 4th Office Sdk Web Service -->
<schema id="SdkWebService"
    targetNamespace="http://www.bcsocial.io/Sdk/WebService"
    elementFormDefault="qualified"
    xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:bc="http://www.bcsocial.io/Sdk/WebService"
>
  
  <!--======================================================================-->
  <!--== Api index =========================================================-->

  <!-- Api Index -->
  <complexType name ="ApiIndex_1">
    <sequence>
      <element name="ProductName" type="string" minOccurs="1" />
      <element name="HttpHeaderList" type="bc:ListOfHttpHeaders_12" minOccurs="0" />
      <element name="UrlParameterList" type ="bc:ListOfApiIndexParameters_1" minOccurs="0" />
      <element name="ApiIndexLinkList" type="bc:ListOfApiIndexLinks_1" minOccurs="1" /> 
    </sequence>
  </complexType>
  <element name="ApiIndex_1" type="bc:ApiIndex_1"/>

  <!-- List of api index links -->
  <complexType name="ListOfHttpHeaders_12">
    <sequence>
      <element name="HttpHeader" type="bc:HttpHeader_12" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <element name="ListOfHttpHeaders_12" type="bc:ListOfHttpHeaders_12"/>
  
  <!-- Http header -->
  <complexType name ="HttpHeader_12">
    <sequence>
      <element name="Name" type ="string" minOccurs="1" />
      <element name="Description" type ="string" minOccurs="1" />
      <element name="Required" type ="boolean" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="HttpHeader_12" type="bc:HttpHeader_12"/>
  
  <!-- List of api index links -->
  <complexType name="ListOfApiIndexLinks_1">
    <sequence>
      <element name="ApiIndexLink" type="bc:ApiIndexLink_1" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <element name="ListOfApiIndexLinks_1" type="bc:ListOfApiIndexLinks_1"/>
  
  <!-- Api Index link -->
  <complexType name ="ApiIndexLink_1">
    <sequence>
      <element name="Name" type ="string" minOccurs="1" />
      <element name="Description" type ="string" minOccurs="1" />
      <element name="Href" type ="string" minOccurs="1" />
      <element name="ContentTypes" type ="bc:ListOfContentTypes" minOccurs="1" />
      <element name="Methods" type ="bc:ListOfApiIndexMethods_1" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="ApiIndexLink_1" type="bc:ApiIndexLink_1"/>
  
  <!-- Content type list -->
  <complexType name ="ListOfContentTypes">
    <sequence>
      <element name="ContentType" type ="string" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfContentTypes" type="bc:ListOfContentTypes"/>

  <!-- Api index options list -->
  <complexType name ="ListOfApiIndexOptions_4">
    <sequence>
      <element name="Options" type ="bc:ApiIndexOptions_1" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfApiIndexOptions_4" type="bc:ListOfApiIndexOptions_4"/>
  
  <!-- Api Index options -->
  <complexType name ="ApiIndexOptions_1">
    <sequence>
      <element name="Name" type="string" minOccurs="1" />
      <element name="Methods" type ="bc:ListOfApiIndexMethods_1" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="ApiIndexOptions_1" type="bc:ApiIndexOptions_1"/>
  
  <!-- Api Index method -->
  <complexType name ="ApiIndexMethod_1">
    <sequence>
      <element name="Type" type ="string" minOccurs="1" />
      <element name="Description" type ="string" minOccurs="1" />
      <element name="Parameters" type ="bc:ListOfApiIndexParameters_1" minOccurs="0" />
      <element name="HttpHeaderList" type="bc:ListOfHttpHeaders_12" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="ApiIndexMethod_1" type="bc:ApiIndexMethod_1"/>
  
  <!-- Api Index method list -->
  <complexType name ="ListOfApiIndexMethods_1">
    <sequence>
      <element name="ApiIndexMethod" type ="bc:ApiIndexMethod_1" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfApiIndexMethods_1" type="bc:ListOfApiIndexMethods_1"/>
  
  <!-- Api Index parameter -->
  <complexType name ="ApiIndexParameter_1">
    <sequence>
      <element name="Type" type ="string" minOccurs="1" />
      <element name="Name" type ="string" minOccurs="1" />
      <element name="Description" type ="string" minOccurs="1" />
      <element name="Required" type ="boolean" minOccurs="1" />
      <element name="IsList" type ="boolean" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="ApiIndexParameter_1" type="bc:ApiIndexParameter_1"/>
  
  <!-- Api Index parameter list -->
  <complexType name ="ListOfApiIndexParameters_1">
    <sequence>
      <element name="ApiIndexParameter" type ="bc:ApiIndexParameter_1" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfApiIndexParameters_1" type="bc:ListOfApiIndexParameters_1"/>

  <!--======================================================================-->
  <!--== Logon =============================================================-->
  
  <!-- Logon abstract -->
  <complexType name ="LogOn">
    <sequence>
      <!-- Service id -->
      <element name="ServiceId" type ="string" minOccurs="1" />
      <!-- Create long lasting cookie for authenticated session. -->
      <element name="KeepMeLoggedIn" type="boolean" minOccurs="0" />
      <!-- After successfull logon this field is populated with access and refresh tokens 
           which can be used for further authorization purposes. -->
      <element name="Token" type ="bc:AccessToken_14" minOccurs="0" />
   </sequence>
  </complexType>
  <element name="LogOn" type="bc:LogOn"/>

  <!-- User logon via mobile device (v4) -->
  <complexType name ="LogOnUserMobile_4">
    <complexContent>
      <extension base="bc:LogOn">
        <sequence>
          <!-- User identity -->          
          <element name="Identity" type ="string" minOccurs="1" />         
          <element name="AuthenticationType" type="bc:AuthenticationType" minOccurs="1" />
          <element name="AuthenticationToken" type="string" minOccurs="0" />
          <element name="ApplicationVersion" type="string" minOccurs="1" />
          <element name="Device" type="bc:MobileDevice_4" minOccurs="0" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="LogOnUserMobile_4" type="bc:LogOnUserMobile_4"/>
      
  <!-- Logon abstract -->
  <complexType name ="LogOnBase_14">
    <sequence>
      <!-- After successfull logon this field is populated with access and refresh tokens 
           which can be used for further authorization purposes. -->
      <element name="Token" type ="bc:AccessToken_14" minOccurs="0" />
   </sequence>
  </complexType>
  <element name="LogOnBase_14" type="bc:LogOnBase_14"/>

  <!-- User logon.
       - Version: 4.0
       - Content types:
         - application/vnd.4thoffice.logon.user-v4.0+xml
         - application/vnd.4thoffice.logon.user-v4.0+json
         -->
  <complexType name ="LogOnUser_14">
    <complexContent>
      <extension base="bc:LogOnBase_14">
        <sequence>
          <element name="Authentication" type="bc:Authentication_13" minOccurs="1" />
          <element name="ClientApplication" type="bc:ClientApplication_14" minOccurs="1" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="LogOnUser_14" type="bc:LogOnUser_14"/>
  
  <complexType name ="ClientApplication_14">
    <sequence>
      <element name="Type" type="bc:ClientApplicationType" minOccurs="1" />
      <element name="Version" type ="string" minOccurs="1" />
      <element name="CodeName" type="string" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="ClientApplication_14" type="bc:ClientApplication_14"/>

  <!-- Type of client application. -->
  <simpleType name="ClientApplicationType">
    <restriction base='string'>
      <enumeration value="Unknown" />
      <enumeration value="Ios" />
      <enumeration value="Android" />
      <enumeration value="WindowsPhone" />
      <enumeration value="Web" />
    </restriction>
  </simpleType>
  <element name="ClientApplicationType" type="bc:ClientApplicationType"/>
      
  <!--======================================================================-->
  <!--== Authentication ====================================================--> 
  
  <!-- Authentication type --> 
  <simpleType name="AuthenticationType">
    <restriction base='string'>
      <enumeration value="Test" />
      <enumeration value="Facebook" />
      <enumeration value="Google" />
      <enumeration value="SdkServiceApiAccess" />
      <enumeration value="LinkedIn" />
      <enumeration value="EmailPassword" />
      <enumeration value="MicrosoftOnline" />
      <!-- 4O integration used for robots, etc. -->
      <enumeration value="Integration" />
      <enumeration value="MicrosoftExchange" />
    </restriction>
  </simpleType>
  <element name="AuthenticationType" type="bc:AuthenticationType"/>
  
  <complexType name ="Authentication_13">
    <sequence>
      <element name="AuthenticationType" type="bc:AuthenticationType" minOccurs="1" />
      <element name="AuthenticationId" type="string" minOccurs="1" />  
      <element name="AuthenticationToken" type="string" minOccurs="0" />
      <element name="AuthenticationProviderUri" type="string" minOccurs="0" />
      <element name="AuthenticationProviderDomain" type="string" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="Authentication_13" type="bc:Authentication_13"/>
  
  <complexType name="ListOfAuthentications_13">
    <sequence>
      <element name="Authentication" type="bc:Authentication_13" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfAuthentications_13" type="bc:ListOfAuthentications_13" />
  
  <simpleType name="AuthenticationSchemaLogonType">
    <restriction base='string'>
      <enumeration value="AuthenticationProviderUri" />
    </restriction>
  </simpleType>
  <element name="AuthenticationSchemaLogonType" type="bc:AuthenticationSchemaLogonType"/>

  <!--======================================================================-->
  <!--== Account ===========================================================-->

  <complexType name ="AccountBase_14">
    <sequence>
      <element name="Id" type="string" minOccurs="0" />
      <element name="Authentication" type="bc:Authentication_13" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="AccountBase_14" type="bc:AccountBase_14"/>

  <complexType name ="AccountEmail_14">
    <complexContent>
      <extension base="bc:AccountBase_14">
        <sequence>
          <element name="Email" type="string" minOccurs="0" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="AccountEmail_14" type="bc:AccountEmail_14"/>
  
  <complexType name="ListOfAccounts_14">
    <sequence>
      <element name="Account" type="bc:AccountBase_14" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfAccounts_14" type="bc:ListOfAccounts_14" />
  
  <!--======================================================================-->
  <!--== Attachment(s) =====================================================--> 
    
  <!-- Attachment (v4) -->
  <complexType name ="Attachment_4">
    <sequence>
      <element name="Id" type="long" minOccurs="0" />
      <element name="IdString" type="string" minOccurs="0" />
      <element name="Version" type="long" minOccurs="0" />
      <element name="File" type ="bc:File_4" minOccurs="0" />
      <!-- Is hidden attachment. 
           If true such attachment is used for preview and download purposes and is not visible to end user. -->
      <element name="Hidden" type="boolean" minOccurs="0" />
      <!-- Service id of attachment. 
           If set it defines document type of that attachment (email, gdoc, etc)-->
      <element name="ServiceId" type="string" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="Attachment_4" type="bc:Attachment_4"/>

   <!-- Attachment list (v1) -->
  <complexType name ="ListOfAttachments_4">
    <sequence>
      <element name="Attachment" type="bc:Attachment_4" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfAttachments_4" type="bc:ListOfAttachments_4"/>
  
  <!--=====================================================================-->
  <!--== File(s) ==========================================================--> 
  
  <!-- File (v1) -->
  <complexType name ="File_1">
    <sequence>    
      <element name="Content" type="base64Binary" minOccurs="0" /> 
      <element name="Name" type ="string" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="File_1" type="bc:File_1"/>

  <!-- File list -->
  <complexType name ="ListOfFiles_1">
    <sequence>
      <element name="File" type="bc:File_1" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfFiles_1" type="bc:ListOfFiles_1"/>

  <!-- File (v4) -->
  <complexType name ="File_4">
    <sequence>
      <!-- File name -->
      <element name="Name" type ="string" minOccurs="1" />
      <!-- File content as base64 encoded string -->
      <element name="Content" type="string" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="File_4" type="bc:File_4"/>
  
  <!-- File list (v4) -->
  <complexType name ="ListOfFiles_4">
    <sequence>
      <element name="File" type="bc:File_4" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfFiles_4" type="bc:ListOfFiles_4"/>
  
  <!-- File
     - Version: 4.0
     - Content types:
      - application/4thoffice.file-4.0+xml
      - application/4thoffice.file-4.0+json
  -->
  <complexType name ="File_14">
    <complexContent>
      <extension base="bc:ResourceBase_13">
        <sequence>
          <element name="ThumbnailSupported" type="boolean" minOccurs="0" />
          <element name="ThumbnailPreview" type="bc:ThumbnailPreview_15" minOccurs="0" />
          <element name="Parent" type="bc:ResourceBase_13" minOccurs="0" />
          <!-- File size [B] -->
          <element name="Size" type ="long" minOccurs="0" nillable="true" />
          <!-- Created date and time-->
          <element name="Created" type="dateTime" minOccurs="0" nillable="true" />
          <!-- Modified date and time -->
          <element name="Modified" type="dateTime" minOccurs="0" nillable="true" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="File_14" type="bc:File_14"/>
  
  <!-- File list (v4) -->
  <complexType name ="ListOfFiles_14">
    <sequence>
      <element name="File" type="bc:File_14" minOccurs="1" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  <element name="ListOfFiles_14" type="bc:ListOfFiles_14"/>
      
  <!--======================================================================-->
  <!--== Document ==========================================================-->
  
  <!-- Document resource
     - Version: 3.6
     - Content types:
       - application/vnd.marg.bcsocial.document-v3.6+xml 
       - application/vnd.marg.bcsocial.document-v3.6+json
    -->
  <complexType name="Document_13">
    <complexContent>
      <extension base="bc:ResourceBase_13">
        <sequence>
          <!-- Id of community where document was created -->
          <element name="CommunityId" type="long" minOccurs="0" />
          <!-- Created date and time-->
          <element name="Created" type="dateTime" minOccurs="0" />
          <!-- Modified date and time -->
          <element name="Modified" type="dateTime" minOccurs="0" />
          <!-- Document title -->
          <element name="Title" type ="string" minOccurs="0" />
          <!-- Document description -->
          <element name="Description" type ="string" minOccurs="0" />
          <!-- Document owner -->
          <element name="Owner" type ="bc:ActorBase_13" minOccurs="0" />
          <!-- Id of service defining the content -->
          <element name="ServiceId" type="string" minOccurs="0" />
          <!-- Document content -->
          <element name="Content" type="bc:Attachment_4" minOccurs="0" /> 
          <!-- Attachment list -->
          <element name="AttachmentList" type="bc:ListOfAttachments_4" minOccurs="0" /> 
          <!-- Is document followed by calling user -->
          <element name="IsFollowed" type="boolean" minOccurs="0" />
          <!-- Is document bookmarked by calling user -->
          <element name="IsBookmarked" type="boolean" minOccurs="0" />
          <!-- Share list -->
          <element name="ShareList" type="bc:ListOfResources_13" minOccurs="0" />
          <!-- Document size [B] -->
          <element name="Size" type ="long" minOccurs="0" nillable="true" />
          <!-- Parent id. If set this document is an attachment. -->
          <element name="ParentId" type ="string" minOccurs="0" /> 
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="Document_13" type="bc:Document_13" />
  
  <!-- Document list resource 
     - Version: 3.6
     - Content types:
       - application/vnd.marg.bcsocial.document.list-v3.6+xml 
       - application/vnd.marg.bcsocial.document.list-v3.6+json 
  -->
  <complexType name="ListOfDocuments_13">
    <sequence>
      <element name="Document" type="bc:Document_13" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <element name="ListOfDocuments_13" type="bc:ListOfDocuments_13"/>
  
  <!-- Document resource
     - Version: 4.0
     - Content types:
       - application/vnd.4thoffice.document-v4.0+xml 
       - application/vnd.4thoffice.document-v4.0+json
    -->
  <complexType name="Document_14">
    <complexContent>
      <extension base="bc:ResourceBase_13">
        <sequence>
          <!-- Created date and time-->
          <element name="Created" type="dateTime" minOccurs="0" nillable="true" />
          <!-- Modified date and time -->
          <element name="Modified" type="dateTime" minOccurs="0" nillable="true" />
          <!-- Document description -->
          <element name="Description" type ="string" minOccurs="0" />
          <!-- Document owner -->
          <element name="Owner" type ="bc:ActorBase_14" minOccurs="0" />
          <!-- Document file -->
          <element name="File" type="bc:File_14" minOccurs="0" /> 
          <!-- Share list -->
          <element name="ShareList" type="bc:ListOfResources_13" minOccurs="0" />
          <!-- Document size [B] -->
          <element name="Size" type ="long" minOccurs="0" nillable="true" />
          <!-- Parent resource. If set this document is an attachment to an exsiting resource. -->
          <element name="Parent" type ="bc:ResourceBase_13" minOccurs="0" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="Document_14" type="bc:Document_14" />
   
  <!-- Document list resource 
     - Version: 4.0
     - Content types:
       - application/vnd.4thoffice.document.list-v4.0+xml 
       - application/vnd.4thoffice.document.list-v4.0+json 
  -->
  <complexType name="ListOfDocuments_14">
    <sequence>
      <element name="Document" type="bc:Document_14" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <element name="ListOfDocuments_14" type="bc:ListOfDocuments_14"/>
  
  <!-- Document list resource with paging 
     - Version: 4.0
     - Content types:
       - application/vnd.4thoffice.document.list.page-v4.0+xml 
       - application/vnd.4thoffice.document.list.page-v4.0+json 
  -->
  <complexType name="ListOfDocumentsPage_14">
    <sequence>
      <element name="Documents" type="bc:ListOfDocuments_14" minOccurs="1" />
      <element name="Paging" type="bc:Paging_13" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="ListOfDocumentsPage_14" type="bc:ListOfDocumentsPage_14" />

  <!-- Document resource
     - Version: 5.8
     - Content types:
       - application/vnd.4thoffice.document-v5.8+xml 
       - application/vnd.4thoffice.document-v5.8+json
    -->
  <complexType name="Document_18">
    <complexContent>
      <extension base="bc:ResourceBase_13">
        <sequence>
          <!-- Created date and time-->
          <element name="Created" type="dateTime" minOccurs="0" nillable="true" />
          <!-- Modified date and time -->
          <element name="Modified" type="dateTime" minOccurs="0" nillable="true" />
          <!-- Document description -->
          <element name="Description" type ="string" minOccurs="0" />
          <!-- Document owner -->
          <element name="Owner" type ="bc:ActorBase_14" minOccurs="0" />
          <!-- Document file -->
          <element name="File" type="bc:File_14" minOccurs="0" /> 
          <!-- Share list -->
          <element name="ShareList" type="bc:ListOfResources_13" minOccurs="0" />
          <!-- Document size [B] -->
          <element name="Size" type ="long" minOccurs="0" nillable="true" />
          <!-- Parent resource. If set this document is an attachment to an exsiting resource. -->
          <element name="Parent" type ="bc:ResourceBase_13" minOccurs="0" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="Document_18" type="bc:Document_18" />
    
  <!-- Document list resource with paging 
     - Version: 3.6
     - Content types:
       - application/vnd.marg.bcsocial.document.list.page-v3.6+xml 
       - application/vnd.marg.bcsocial.document.list.page-v3.6+json 
  -->
  <complexType name="ListOfDocumentsPage_13">
    <sequence>
      <element name="Documents" type="bc:ListOfDocuments_13" minOccurs="1" />
      <element name="Paging" type="bc:Paging_13" minOccurs="0" />
    </sequence>
  </complexType>
  <element name="ListOfDocumentsPage_13" type="bc:ListOfDocumentsPage_13" />

  <!-- Share list for document.
     - Version: 4.0
     - Content types:
       - application/vnd.4thoffice.document.share.list-v4.0+xml 
       - application/vnd.4thoffice.document.share.list-v4.0+json
    -->
  <complexType name ="DocumentShareList_14">
    <sequence>
      <element name="ShareList" type ="bc:ListOfResources_13" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="DocumentShareList_14" type="bc:DocumentShareList_14"/>
    
  <!-- Pinned-to list for document.
     - Version: 5.0
     - Content types:
       - application/vnd.marg.bcsocial.document.pinnedto.list-v5.0+xml 
       - application/vnd.marg.bcsocial.document.pinnedto.list-v5.0+json
    -->
  <complexType name ="DocumentPinnedToList_15">
    <sequence>
      <element name="BoardList" type ="bc:ListOfBoards_15" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="DocumentPinnedToList_15" type="bc:DocumentPinnedToList_15"/>
   
  <!-- Thumbnail size -->
  <simpleType name="ThumbnailSize">
    <restriction base="string">
      <enumeration value="Small"/>
      <enumeration value="Large"/>
      <enumeration value="Medium"/>
    </restriction>
  </simpleType>
  <element name="ThumbnailSize" type="bc:ThumbnailSize"/>

  <!--======================================================================-->
  <!--== Document thumbnail ================================================-->
  
  <!-- Document thumbnail file (v4) -->
  <complexType name ="DocumentThumbnail_4">
    <sequence>
      <!-- Document id -->
      <element name="DocumentId" type="long" minOccurs="1" />
      <!-- Thumbnail size -->
      <element name="Size" type="bc:ThumbnailSize" minOccurs="1" />
      <!-- Thumbnail file -->
      <element name="File" type="bc:File_4" minOccurs="0" />
      <!-- Number of thumbnail pages -->
      <element name="NumberOfPages" type="int" minOccurs="1" />
      <!-- Current thumbnail page -->
      <element name="CurrentPage" type="int" minOccurs="1" />
    </sequence>
  </complexType>
  <element name="DocumentThumbnail_4" type="bc:DocumentThumbnail_4"/>
  
  <!-- Thumbnail page info (v13) -->
  <complexType name ="ThumbnailPageInfo_13">
    <sequence>
      <!--Page width-->
      <element name="Width" type="int" minOccurs="1" />
      <!--Page height-->
      <element name="Height" type="int" minOccurs="1" />
      <!--Has text map-->
      <element name="HasTextMap" type="boolean" minOccurs="0" nillable="true" />
    </sequence>
  </complexType>
  <element name="ThumbnailPageInfo_13" type="bc:ThumbnailPageInfo_13"/>
  
  <!-- List of thumbnail page info's (v13) -->
  <complexType name="ListOfThumbnailPageInfo_13">
    <sequence>
      <element name="ThumbnailPageInfo" type="bc:ThumbnailPageInfo_13" minOccurs="1" maxOccurs="unbounded"/>
    </sequence>
  </complexType>
  <element name="ListOfThumbnailPageInfo_13" type="bc:ListOfThumbnailPageInfo_13"/>
  
   <!-- Document thumbnail page info
      - Version: 3.6
      - Content types:
        - application/vnd.marg.bcsocial.document.thumbnail.page.info-v3.6+xml 
        - application/vnd.marg.bcsocial.document.thumbnail.page.info-v3.6+json 
    -->
